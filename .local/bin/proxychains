config=~/.proxychains/proxychains.conf

protocol=socks5
ip=$(grep 'nameserver' /etc/resolv.conf | cut -d' ' -f2)
port=1082

rm -f $config

cat > $config << EOF
dynamic_chain
proxy_dns
remote_dns_subnet 224
tcp_read_time_out 15000
tcp_connect_time_out 8000
[ProxyList]
$protocol $ip $port
EOF

/bin/proxychains $@
